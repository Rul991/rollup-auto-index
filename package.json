{
  "name": "rollup-auto-index-ts",
  "version": "1.0.0",
  "main": "src/index.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "watch": "nodemon --ext ts -w src --exec \"npm start\"",
    "build": "rollup -c",
    "start": "npm run build && node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["rollup", "typescript", "auto", "index", "file"],
  "author": "Rul",
  "license": "ISC",
  "description": "TypeScript Rollup plugin for automatically generating an index file",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.3",
    "@types/rollup": "^0.51.4",
    "rollup": "^4.40.1",
    "tslib": "^2.8.1"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rul991/rollup-auto-index"
  },
  "publishConfig": {
    "access": "public"
  }
}
